@thesis{opillard_we_2018,
	title = {"We shall not be moved - El barrio no se vende". Analyse critique des mobilisations contre la gentrification à San Francisco (États-Unis) et contre la prédation immobilière à Valparaíso (Chili)},
	rights = {Licence Etalab},
	url = {https://www.theses.fr/2018PSLEH174},
	abstract = {Cette recherche porte sur les mobilisations urbaines contre les effets des politiques dites néolibérales, dans les villes de San Francisco et de Valparaíso. L’enjeu est de comprendre comment se structurent dans ces deux espaces urbains des collectifs de lutte non-institutionnels, qui participent à des degrés d’intensité divers à la construction de coalitions urbaines pour influer sur les rapports de pouvoir dans la production urbaine. Ce travail s’inscrit dans une démarche transdisciplinaire. Tout en étant ancré dans la géographie, il interroge les modalités de structuration d’actions collectives à partir de méthodes et de questionnements de l’anthropologie et de la sociologie de l’action collective. La démarche est par ailleurs résolument comparative, elle prend le parti d’une analyse des réponses organisées à des politiques globales dans deux cas spécifiques, pour en souligner les facteurs de différenciation et les points communs. Dans un premier temps, l’analyse propose de rentrer de plain-pied dans les luttes urbaines par des récits de situations d’engagement autour de motifs communs dans les deux cas : la dépossession des habitant.es de leur logement et la privatisation des infrastructures urbaines. Cette thèse approfondit ensuite les caractéristiques des groupes en question, pour les réinsérer dans leurs contextes militants et urbains respectifs. Enfin, cette recherche souligne l’adaptation des formes d’action collective à la financiarisation de l’urbain : d’une part la transformation des répertoires d’action, dans lesquels les militant.es s’attaquent spécifiquement aux capitaux symboliques des acteurs dominants. D’autre part, la forme même des « collectifs militants » représente des formes d’adaptation à la pénétration des rationalités néolibérales.},
	institution = {Paris Sciences et Lettres ({ComUE})},
	type = {Thèse de doctorat},
	author = {Opillard, Florian},
	editora = {Musset, Alain and Ghorra-Gobin, Cynthia},
	editoratype = {collaborator},
	urldate = {2024-01-02},
	date = {2018-12-12},
	keywords = {Anti-Eviction Mapping Project, Cerro Barón Organizado, Comparaison, Comparative approach, Dimension spatiale, Droit à la ville, Embourgeoisement (urbanisme) -- États-Unis -- San Francisco (Calif.), Espaces publics -- Chili -- Valparaíso (Chili ; région), Espaces publics -- États-Unis -- San Francisco (Calif.), Financialization, Financiarisation, Libéralisme économique, Mouvements sociaux, Neoliberalism, Néolibéralisme, Right to the city, San Francisco, Social movements, Spatial dimension, Spéculation foncière -- Chili -- Valparaíso (Chili ; région), Valparaíso},
