@book{bourdeau-lepage_reveler_2022,
	title = {Révéler les aspirations des citadins pour des villes amènes. L'outil Tell\_Me},
	abstract = {Il y a, déjà 10 ans, nous plaidions pour une « ville douce, sensible à l'Humain », une ville plus juste, qui respecte les différences entre les personnes qui composent notre société ; une ville qui tienne compte des aspirations, des besoins et des rythmes de ses habitants et respecte son environnement » (Bourdeau-Lepage, 2012). Aujourd'hui, avec la reconnaissance généralisée des dommages causés par l'activité humaine sur l'environnement (destruction des écosystèmes, perte de biodiversité…) et sur le climat, la plus grande prise de conscience par les citadins des bienfaits de la nature sur leur bien-être et leur santé (Bourdeau-Lepage, 2019), ainsi que le verdissement sociétal, cette proposition semble davantage à propos. Ainsi, la ville devrait être envisagée comme un lieu de bien-être et d'épanouissement des personnes. Nous devons par conséquent changer notre manière de nous localiser et d'aménager notre espace de vie en gardant en tête qu'il nous faut préserver la santé des animaux et des écosystèmes car notre santé et notre bien-être en dépendent (Woods et al., 2018). Mais comment pouvons-nous opérer ce changement ? Disposons-nous d'outils qui nous permettent de le faire ? La réponse est oui. Nous disposons notamment de travaux menés, par les chercheurs sur le bien-être et sur les effets de l'environnement sur la santé des citadins (voir le chapitre d'Albert Lévy dans cet ouvrage) qui sont d'une grande aide. Il s'agit donc de changer d'angle d'attaque et de placer le bien-être des citadins au coeur des politiques publiques urbaines. De nombreux avantages existent à analyser le bien-être sur un territoire, en particulier si nous reconnaissons que le but des politiques publiques est a priori de faire en sorte que les personnes vivent le mieux possible. A l'échelle locale, il s'agit pour les acteurs territoriaux de mener des actions qui conduisent à une meilleure adéquation entre les demandes des habitants et les potentialités de leur territoire, dans le but d'améliorer la situation de leurs concitoyens. Pour ce faire, ils doivent connaître : i) les attentes et les préoccupations des habitants, ii) repérer les atouts et les faiblesses de leur territoire, iii) comprendre comment se construit le bien-être et quels en sont ses déterminants spatiaux. C'est justement ce que permettent certaines approches sur le bien-être (Cf. la section qui suit pour le panorama). Une de ces approches a été élaborée au milieu des années 2010.},
	pagetotal = {13},
	author = {Bourdeau-Lepage, Lise},
	date = {2022-03-19},
	langid = {french},
	keywords = {Lu (à ficher)},
}

