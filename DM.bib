@article{li_bicycling_2018,
	title = {Bicycling Accessibility Based on Dock-Less Bicycle-Sharing Data},
	volume = {1377},
	rights = {http://rightsstatements.org/page/{InC}-{NC}/1.0/},
	url = {https://www.research-collection.ethz.ch/handle/20.500.11850/284832},
	doi = {10/gp8dz6},
	abstract = {Accessibility for non-motorized traffic is an important topic in the transportation. To obtain the measures of bicycle accessibility, we estimated the impedance function for different purposes using the Shanghai dock-less bicycle-sharing data. In specific, as the bicycling data have no trip purpose information, the information was imputed by a Dirichlet-multinomial regression topic model ({DMR}), which could consider the arriving time and drop-off location simultaneously. The imputed results were validated by displaying the trip purposes on the visualized map in Shanghai central area. Then the impedance functions of the bicycling accessibility model were further estimated for the trips with different purposes based on the trips’ distance and duration. We found the descriptive values of the real trips and parameters of the impedance functions for different trip purposes were different. In addition, almost 90\% of trips were less than 30 minutes and 5 kilometers. Finally, two cases of bicycling accessibility with two different purposes (restaurant and hospital), were illustrated in the central area of Shanghai based on the estimated impedance function. This study demonstrates the feasibility of combining bicycle-sharing {GPS} data and the {DMR} model to estimate the bicycle accessibility. Moreover, the estimated models for different purposes can benefit for urban planning and development, such as bicycle-based transit-oriented development and land use-transportation planning evaluation.},
	journaltitle = {Arbeitsberichte Verkehrs- und Raumplanung},
	author = {Li, Aoyong and Huang, Yizhe and Axhausen, Kay W.},
	urldate = {2022-05-30},
	date = {2018-07},
	langid = {english},
	note = {Accepted: 2018-08-27T12:43:22Z
Publisher: {IVT}, {ETH} Zurich},
	keywords = {A lire et ficher, Systematic\_Review},
}

@incollection{georgiadis_shared_2020,
	edition = {Springer, Cham},
	title = {Shared Mobility and Last-Mile Connectivity to Metro Public Transport: Survey Design Aspects for Determining Willingness for Intermodal Ridesharing in Athens},
	isbn = {978-3-030-58802-1},
	series = {{ICCSA} 2020},
	shorttitle = {Shared Mobility and Last-Mile Connectivity to Metro Public Transport},
	abstract = {People living in peri-urban low-density areas may not choose the urban rail because they are hindered by the ‘first/last mile’ problem. The issue concerns poor bus feeder service to rail stations and/or congested Park\&Ride facilities at respective intermodal hubs. Shared mobility in the form of carpooling is a viable alternative in connection to urban rail, especially when appropriate incentives and ridematching tools are effectuated. A multi-modal ride-matching app combining flexible (carpooling) and scheduled (rail and bus public transport) mobility is stipulated by the Horizon 2020 Ride2Rail project. Two intermodal hubs of urban rail along the 20 km-long corridor connecting the Athens basin with the Athens airport in Eastern Attica, Greece, are selected as a case study. The paper envisages the behavioural underpinning of combined rail-rideshare travel companion platform in the first/last mile context through the design of a {SP} experiment, as mode choice is concerned. All main access and egress modes of intermodal hubs are considered in the mode choice experiment, namely driving alone, using bus feeder, carpool driving and carpool riding. Tested parameters pertain particularly to incentive mechanisms increasing ridesharing to intermodal hubs, contextual preferences related to the trip purpose and perceived barriers of shared mobility.},
	pages = {819--835},
	booktitle = {Computational Science and Its Applications},
	author = {Georgiadis, Georgios and Deloukas, Alex and Papaioannou, Panagiotis},
	date = {2020-10-02},
	langid = {english},
	doi = {10.1007/978-3-030-58802-1_59},
	keywords = {A lire et ficher},
}

@inproceedings{zhao_impacts_2019,
	title = {Impacts of Intermodal Connection by Bicycle on Service Coverage of Urban Rail Transit},
	url = {https://ascelibrary.org/doi/10.1061/9780784482292.456},
	doi = {10/gr5q2s},
	abstract = {Urban rail transit is a way of framing traffic, and the distribution of its sites is relatively sparse. More and more people choose a bicycle to finish their trips. The study of using a bicycle to connect benefits its management. It also can help optimize the structure of the public transport network. A questionnaire survey at the Sanshanjie Station and Zhonghuamen Station, Nanjing Metro Line 1, was conducted. Based on a previous study on factors influencing the choice of bicycle, this paper further studied the factors influencing the connection range. The study finds that bicycle connection range is not affected by many factors like sex, age, or travel purposes, while it varies with the sites. This paper also establishes a model for calculating the range of bicycle connection in radiation area of urban rail transit. It is verified that the model can be used to calculate the range correctly.},
