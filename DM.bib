@report{guelton_cite-jardin_2013,
	title = {La cité-jardin dans le Grand Paris. Du quartier modèle d’habitations à bon marché à un dispositif structurant dans le zoning métropolitain},
	url = {https://hal.science/hal-04665797},
	abstract = {En affirmant qu’ « il faut créer des cités-jardins dans la banlieue », les auteurs du rapport de la commission d’extension de Paris valident, en 1913, une forme urbaine connue et discutée depuis son importation en France dix ans plus tôt par le juriste Georges Benoit-Lévy. Entendue dans une acception large, dépassant la définition originelle howardienne, la cité-jardin séduit et est présentée par les réformateurs sociaux, à la fois comme une solution à la crise du logement et à l’extension anarchique des périphéries urbaines. Les auteurs du rapport, en l’occurrence Louis Bonnier, proposent ainsi, dans le volume consacré aux considérations techniques préliminaires, de combiner la cité-jardin et le park system, en aménageant deux grands parcs pourvus de deux cités-jardins, au sud et au nord-est de Paris, intégrés dans un vaste système de parcs, suivant le modèle viennois. Après la guerre, la cité-jardin est de nouveau sollicitée dans le programme du concours pour l’établissement du plan d’aménagement et d’extension de Paris de 1919, à la fois comme modèle d’aménagement des zones résidentielles et pour la réutilisation des forts déclassés de la première ligne de défense. Si le concours ne sera suivi d’aucune réalisation, les cités-jardins de l’Office d’{HBM} du département de la Seine en seront l’application partielle et font désormais partie de notre patrimoine.},
	pages = {213--328},
	author = {Guelton, Mayalène},
	urldate = {2025-01-07},
	date = {2013},
	langid = {french},
	doi = {10.25580/igp.2013.0010},
	keywords = {Cité-Jardin, Inventer le Grand Paris},
}

