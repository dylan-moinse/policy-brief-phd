	note = {Publisher: {INRETS} [1984-2010] - {IFSTTAR} [2011-2019] – Université Gustave Eiffel [2020-łdots] with the support of Dunod, Elsevier, Lavoisier, Springer and {NecPlus}},
	keywords = {{PDAU}, {POS}, {TOD}, axis contract, contrat d'axe, interchange hubs, mobility, mobilité, pôle d'échange, tramway},
}

