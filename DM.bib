@book{berg_wincent_access_2022,
	title = {Access Distance to E-scooters: Analysis of Trip and App Use Data in Stockholm},
	shorttitle = {Access Distance to E-scooters},
	abstract = {Access distance to micromobility services is an important component of usage patterns and input to determine the catchment area for the service. The latter is increasingly relevant as policymakers consider regulating shared free-floating e-scooters to designated parking zones. The access distances of e-scooters users in Stockholm, Sweden, are studied through data analysis of e-scooter trip and app use data for one week in May 2019. The walking distance is determined by calculating the Euclidean distance between the location where the users opened the app to search for an e-scooter and the trip's origin. The access distances are analyzed based on time, day, distance to public transportation, and spatial distribution. Users walk on average 147 m and 4.3 minutes with a median value of 77 m and 2.7 minutes. Shorter walking distances are observed for trips during the morning and lunch hours compared to the afternoon and at night. Furthermore, users walked slightly longer during the weekend compared to weekdays. Access distances are shortest within 0-100 m to the nearest public transportation station. The suggested catchment area for shared e-scooters ranges from a radius of 128 m to 203 m, based on the 75th percentile of access distances.},
	author = {Berg Wincent, Boel and Jenelius, Erik and Burghout, Wilco},
	date = {2022-08-01},
	doi = {10.13140/RG.2.2.25002.49608},
}

