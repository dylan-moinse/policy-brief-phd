	doi = {10/gnnf2r},
	shorttitle = {Electric scooter sharing},
	abstract = {With its advantages in urbanized areas, the electric scooter sharing ({ESS}) system has received considerable popularity as a transportation mode for short-distance trips. In this paper, people’s preferences toward {ESS} for last-mile trips were analyzed using a stated preference experiment. We designed the experiment to scrutinize how people value {ESS}-riding compared with other conventional last-mile transportation modes. The values of travel time components such as access and riding times are estimated using a mixed logit model. The values of riding transportation modes were significantly greater than that of walking for a last-mile trip. However, none of the time values of two modes—those from a conventional last-mile mode and {ESS}—was dominating because the difference in the dispersions of each made the time value curves intersect around their medians. Considering the results, we have concluded that {ESS} is a competitive transportation mode in last-mile situations.},
	pages = {102642},
	journaltitle = {Transportation Research Part D: Transport and Environment},
	shortjournal = {Transportation Research Part D: Transport and Environment},
	author = {Baek, Kwangho and Lee, Hyukseong and Chung, Jin-Hyuk and Kim, Jinhee},
	urldate = {2023-05-01},
	date = {2021-01-01},
	langid = {english},
	keywords = {A lire et ficher, Electric scooter sharing, Last-mile transportation, Mixed logit model, Shared micro-mobility, Stated Preference Experiment, Systematic\_Review, Value of travel time},
}

@inproceedings{bechstein_cycling_2010,
	location = {Pretoria, Afrique du Sud},
	title = {Cycling as a Supplementary Mode to Public Transport: a Case Study of Low Income Commuters in South Africa},
	rights = {University of Pretoria},
	isbn = {978-1-920017-47-7},
	url = {https://repository.up.ac.za/handle/2263/14739},
	shorttitle = {Cycling as a supplementary mode to public transport},
	abstract = {Biking is an active, environmentally friendly, and sustainable transportation mode that facilitates short utilitarian and recreational trips. As such, cities around the world have been implementing bicycle sharing/renting programs at stations in a wide network so that a traveler can mount one close to their origins and drop it off at a station nearest to their destinations. In addition, bike sharing programs can solve the first-mile and last-mile connectivity problems of transit modes. Such programs also enable short trips that were previously impossible without using transit modes. This research investigated the relationship between New York City ({NYC}) subway ridership and the {NYC} bike sharing program known as Citi Bike. This study adopted the Spatial Lag Model ({SLM}) to explore the spatial dependencies between variables; moreover, the Ordinary Least Square ({OLS}) model was applied to compare the relative performance of {SLM}. The {SLM} coefficient estimates indicate that bike sharing trips around a subway station were positively associated. The number of bike sharing stations were negatively associated with subway ridership. This study’s findings can be used to develop policies and initiatives that will help to better integrate bike sharing programs with transit services to increase the attractiveness of bike sharing programs and maximize subway ridership.},
	eventtitle = {29th Annual Southern African Transport Conference},
	pages = {9},
	publisher = {Document Transformation Technologies},
	author = {Bechstein, Eva},
	urldate = {2023-04-27},
	date = {2010},
	langid = {english},
