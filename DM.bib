@unpublished{seisenberger_engaging_2023,
	title = {Engaging Citizens and Practitioners: How the 'Flowers of Proximity' Make the 15-Minute City Blossom},
	url = {https://zenodo.org/records/7752547},
	shorttitle = {Engaging Citizens and Practitioners},
	abstract = {The 15-Minute City is often conceived as a human-centred planning ideal. To live up to the ideal, the viewpoints of citizens but also planning practitioners should be consulted. In this contribution, we want to share our experience with a method that we used to initiate discussions about the 15-Minute City: the ‘Flowers of Proximity'.   The Flowers of Proximity were developed by Ana Gil Solá and Bertil Vilhelmson (2019) to discuss the role of proximity in sustainable urban planning. It involves participants locating different destinations such as primary schools, parks, and supermarkets within concentric circles that indicate different travel times from a home location. We adapted the method in workshops on the 15-Minute City with planning practitioners and citizens, initially to identify relevant destinations for accessibility analyses and the willingness to walk to these destinations. However, the Flowers of Proximity helped to stimulate discussions that go far beyond simple quantifications.   As part the development of a guideline for the implementation of the 15-Minute City concept, we used the tool in multiple workshops with planning practitioners (see Büttner et al. 2022, p. 35-36). Especially in our workshop with practitioners from the metropolitan region of Munich, the discussions quickly shifted from the question of how far destinations should be located from people's homes to questions of co-location of services and trip chaining. Having a general practitioner or a pharmacy within 15 minutes is important, but the participants also noted that these destinations should be in proximity to each other. Similarly, many practitioners reflected on the need of locating retirement homes in central locations to ensure the inclusion of vulnerable residents.   In another project in which we developed an accessibility tool for citizens (https://citizens.plan4better.de/), we used the Flowers of Proximity in a workshop with 14 residents from Munich. The method enabled people to talk about their ideal 15-Minute City and reflect on their personal travel behaviour. The participants raised concerns regarding basic assumptions of the 15-Minute City concept such as the focus on walking and cycling and the neglect of public transport. Furthermore, questions about the quality of the destinations were raised. For example, having a park within 15 minutes is not enough; it must also suite the individuals' needs and preferences.  The lively discussions as well as the appealing visual results made the Flowers of Proximity a valuable tool in our projects. Applying the method in different countries and with people of different backgrounds can help to sharpen the concept of the 15-Minute City, adapt it to different local contexts, and deliver the promise of a human-centred planning ideal.  References  Büttner, B., Seisenberger, S., Baquero, M., Rivas, A., Haxhija, S., Ramirez, A., \& {McCormick}, B. (2022). ±15-Minute City: Human-centred planning in action. {EIT} Urban Mobility: Urban Mobility Next 9. https://www.eiturbanmobility.eu/wp-content/uploads/2022/11/{EIT}-{UrbanMobilityNext}9\_15-min-City\_144dpi.pdf  Gil Solá, A., \& Vilhelmson, B. (2019). Negotiating proximity in sustainable urban planning: A Swedish case. Sustainability (Switzerland), 11(1). https://doi.org/10.3390/su11010031},
	author = {Seisenberger, Sebastian and Buettner, Benjamin},
	urldate = {2023-12-23},
	date = {2023-03-08},
	doi = {10.5281/zenodo.7752547},
	keywords = {15 minute city, co design, flowers of proximity, inclusive planning, proximity centred accessibility, public participation},
}

@book{maresca_photographie_1996,
	location = {Paris},
	title = {La photographie, un miroir des sciences sociales},
	isbn = {978-2-7384-4774-6},
	abstract = {Au tournant des années trente, plusieurs photographes ont commencé à utiliser les portraits dans une optique documentaire. Cette torsion du genre n'allait pas de soi : comment les photographies d'individus fixés dans leur singularité pouvaient-elles devenir les portraits de groupes sociaux ou d'une société tout entière ? Une telle transformation soulève des questions sur les rapports du particulier au général, sur les notions d'exemplarité, de représentativité, sur les conditions de généralisation, etc. Or, dans le même temps où la photographie documentaire s'inspirait plus ou moins librement des sciences sociales, ces dernières se détournaient de l'image pour mieux s'affirmer dans les enceintes académiques. De ce divorce, sont issues les disciplines que nous connaissons aujourd'hui, années du seul discours écrit et fermées aux données visuelles et, plus largement, sensorielles. Constater la rupture est une chose, la dépasser en est une autre. Ce livre se propose d'explorer quelques pistes ouvertes par la confrontation de la démarche artistique, celle des photographes en particulier, et de la démarche scientifique. Pour peu, en effet, que l'on prête intérêt à la manière dont l'art et la science se font, et non plus seulement dont ils se pensent, il devient pertinent de décloisonner les différents regards qui se croisent sur les mêmes réalités et, au risque de bousculer les hiérarchies intellectuelles, de s'appliquer à réfléchir les sciences sociales dans le miroir des images. Au bout du compte, l'enjeu serait de réussir à penser aussi avec les yeux.},
	pagetotal = {268},
	publisher = {Editions L'Harmattan},
	author = {Maresca, Sylvain},
	date = {1996-01-01},
}

