	editoratype = {collaborator},
	urldate = {2025-02-09},
	date = {1997-01-01},
	keywords = {Infrastructures de transport, Réseaux (aménagement du territoire), Transport -- Modèles mathématiques, Transport -- Systèmes d'information},
