@thesis{kang_university_2020,
	title = {University Students’ Adoption and Post-Adoption Behaviors of New Mobility Services in China : Three Empirical Essays},
	url = {https://theses.hal.science/tel-03331932},
	shorttitle = {University students’ adoption and post-adoption behaviors of new mobility services in China},
	abstract = {Over the last decade, new mobility services ({NMS}) such as free-floating bike-sharing services ({FFBSS}, like Ofo), ride-sourcing services ({RSS}, like Uber) have expanded rapidly around the world. However, in the field of transport studies, literature on adoption and post-adoption behaviors of these {NMS} and their influential factors are still limited and fragmentary. This thesis aims to advance our understanding of these topics through three empirical essays by focusing firstly on Chinese university students, and secondly on two types of {NMS} (i.e. {FFBSS} and {RSS}).Results are based on an original survey conducted in Nanjing University in November 2017 on university students’ daily travel behaviors, and on current and future usages of new mobility services. Data show that about 90\% of respondents have used {FFBSS} in Nanjing, and over 10\% of respondents have chosen {FFBSS} as their main mode of transport for both commuting and non-commuting trips. More than 60\% of respondents have used {RSS} in Nanjing, but only approximately 60\% of {RSS} users have a positive continuance intention towards the use of these services in the near future. To understand adoption and post-adoption behavior on {NMS} and their influential factors, the thesis is composed of three empirical essays.The first essay examines the factors influencing the frequency of use of {FFBSS} by using a {GOL} (generalized ordered logit) model. The second essay studies the role of {FFBSS} in students’ mode choices for commuting and shopping trips and their influential factors via two {MNL} (multinominal logit) models. The third essay uses two {PMSS} (probit model with sample selection) models to investigate the factors that influence the likelihood of use of {RSS} and users’ continuance intention},
	institution = {Université Paris-Est},
	type = {phdthesis},
	author = {Kang, Wei},
	editora = {Aguiléra, Anne and Rallet, Alain},
	editoratype = {collaborator},
	urldate = {2024-12-30},
	date = {2020-12-04},
	langid = {english},
}

