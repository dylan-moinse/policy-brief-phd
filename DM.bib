@thesis{souchon_intermodalite_2006,
	title = {De l'intermodalité à la multimodalité : enjeux, limites et perspectives, illustré par un projet d'expérimentation d'une tarification multimodale entre Martigues et Marseille},
	url = {https://dumas.ccsd.cnrs.fr/dumas-00412863},
	shorttitle = {De l'intermodalité à la multimodalité},
	abstract = {La Direction Déléguée {TER} {PACA} de la {SNCF} souhaiterait mettre en place une tarification multimodale car/train entre Martigues et Marseille. Ce projet serait une expérimentation destinée à tester la faisabilité et la pertinence d'une telle tarification. En cas de succès, il est envisagé d'étendre l'opération sur d'autres sites. Le choix s'est porté sur la liaison Marseille-Martigues, notamment pour qualité des relations entre les principaux acteurs impliqués dans le projet : la {SNCF}, {KEOLIS} (exploitant pour le compte du {CG}13 de la ligne de car Martigues-Marseille) et la régie martégale des transports urbains « Bus du Soleil ». Pour quantifier et étudier la population cible, deux enquêtes ont été réalisées ; l'une visait la clientèle des cars interurbains, l'autre celle des Trains Express Régionaux, entre Marseille et Martigues. Il en est ressorti un potentiel de clients multimodaux faible, mais suffisant pour lancer l'expérimentation sans risques financiers trop importants. Les principales difficultés identifiées sont liées à la multiplicité des acteurs impliqués, à la distribution à prévoir, et à la compatibilité techniques des titres de transport. En intégrant ces contraintes, l'étude préconise la mise en place de deux titres multimodaux à destination des migrants quotidiens, l'un hebdomadaire et l'autre mensuel, pour des prix de vente légèrement supérieurs aux prix actuels, afin de valoriser le choix modal offert au client. La mise en place de l'expérimentation est prévue pour 2007, après soumission du projet aux Autorités Organisatrices concernées.},
	institution = {Société Nationale des Chemins de Fers Français [{SNCF}], Direction Déléguée {TER}, Provence Alpes Côte D'Azur},
