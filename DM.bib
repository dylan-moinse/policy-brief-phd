	author = {Moreau, Hélie and Jamblinne de Meux, Loïc de and Zeller, Vanessa and D’Ans, Pierre and Ruwet, Coline and Achten, Wouter M. J.},
	urldate = {2021-02-16},
	date = {2020-01},
	langid = {english},
	note = {Number: 5
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {Europe, Impact environnemental, Intégré dans l'état de l'art synthétique (e-scooters), Owned e-scooters, Revue de litterature, Shared e-scooters, Trottinettes électriques partagées ({TEFF}), Trottinettes électriques personnelles, e-scooter, environmental assessment, life cycle assessment, mobility, product-service system},
