@inproceedings{zuniga-garcia_dockless_2020,
	location = {Washington, {DC}},
	title = {Dockless Electric Scooters and Transit Use in an Urban/University Environment},
	abstract = {Micromobility services presented an exponential growth in recent years due to the introduction of shared electric dockless scooter (e-scooters) services in cities across the United States. E-scooters offer an alternative for short trips and are particularly suitable for solving the first-mile- last-mile transit access and egress problem. However, this emerging transportation technology has brought multiple challenges to urban areas, including the lack of infrastructure, deficient operating rules and regulations, and safety concerns. There is a lack of research on their impact on the urban environment. The main challenge remains in the availability of data. The principal objective of this research is to analyze e-scooter trips and interactions with transit in an urban/university environment. We make use of publicly available datasets to describe trip patterns in a six-month term in the City of Austin. We aggregate the information by traffic analysis zones and evaluate the key variables influencing e-scooters trip origins and destinations using a spatial error model ({SEM}) to account for spatial autocorrelation. Additionally, we use a campus-wide survey to evaluate university e-scooter usage and to explore population characteristics, mode shift, mode interaction, and opinions toward new e-scooter policies and regulations implemented in the university. Principal findings suggest that there is no enough evidence of interaction between e-scooter and transit trips. In the university environment, the mode interaction is not significant, and instead, there is a presence of mode shift between e-scooters and transit.},
	eventtitle = {99th Annual Meeting of the Transportation Research Board},
	pages = {17},
	author = {Zuniga-Garcia, Natalia and Machemehl, Randy},
	urldate = {2021-05-06},
	date = {2020-01-14},
	langid = {american},
	keywords = {A lire et ficher, Combined, Shared e-scooters, Transports en commun, Trottinettes électriques partagées ({TEFF})},
}

