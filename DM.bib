@inproceedings{hartanto_developing_2017,
	title = {Developing a Bikeability Index in the Context of Transit-Oriented Development ({TOD})},
	abstract = {The assessment of {TOD} regarding cycling infrastructure is commonly looked at in combination with pedestrian networks. However, we argue that these are different in nature and scale. Exploring specific indicators would then contribute to a more thorough evaluation of the extent to which a {TOD} environment is cyclable (or walkable). This study therefore develops a bikeability index in the context of {TOD}. Literature review on methods to measure bikeability supported the selection of indicators that are appropriate in a {TOD} context. The index was demonstrated for 21 train stations in the Dutch Arnhem-Nijmegen region. Results indicate clear differences between urban and suburban stations. This is especially relevant for policy makers when the interest is on improving particular indicators related to the infrastructural provision that aids to the development of policies
for non-motorized transport in the context of {TOD}.},
	author = {Hartanto, Kurniawan and Grigolon, Anna Beatriz and Maarseveen, M.F.A.M. and Brussel, Mark},
	date = {2017},
	keywords = {A lire et ficher},
}

@article{harpalete_estimating_2023,
	title = {Estimating Travel Demand with a Multimodal Transport Model Including e-Scooters in Iași, Romania},
	volume = {35},
	issn = {1848-4069, 0353-5320},
	url = {http://traffic2.fpz.hr/index.php/PROMTT/article/view/182},
	doi = {10/gshf3b},
	abstract = {Cities, even medium-sized and small ones, have become overwhelmed by traffic and congestion. Innovative solutions are required and recent studies have focused on sustainable approaches. This study aims to develop a multimodal transport model consisting of an urban public transportation ({UPT}) service combined with an e-scooter sharing service. The main purpose of the model is a comparison between two travel alternatives (car and {UPT} + e-scooter), calculating their associated levels of utility. The methodology is based on a multinomial logit model, implemented in Matlab software, using results from an online mobility survey (socio-economic characteristics of potential users of e-scooters). The additional aim is related to the assessment of the inhabitants’ availability to shift from car to multimodal service or to simple e-scooter service. The developed micromodel was applied in Iași, a mid-sized city located in the northeast of Romania. Several price strategies were analysed in order to find their influence on users’ mobility behaviour. It turned out that the price for the shared e-scooter is too high at this moment to be used for daily travel. Without a price decrease and public authorities’ involvement in facilitating the multimodality, the e-scooter remains only an entertainment activity.},
	pages = {349--363},
	number = {3},
	journaltitle = {Promet - Traffic\&Transportation},
	shortjournal = {{PROMTT}},
	author = {Harpalete, Ovidiu-Laurențiu},
	urldate = {2023-07-19},
	date = {2023-06-28},
	keywords = {A lire et ficher, Systematic\_Review},
}

