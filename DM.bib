@book{joseph_villes_1999,
	title = {Villes en gares},
	isbn = {978-2-87678-484-0},
	abstract = {Ce livre interroge l'urbanité des grands pôles d'échanges de la ville d'aujourd'hui : il analyse, et observe, le caractère public de l'espace transport comme prolongement de la rue. À rebours de l'histoire ferroviaire qui fait de la gare le " bâtiment-voyageurs ", il cherche dans " les pôles d'échange " des dispositifs de densification des échanges et d'expansion des mobilités urbaines. Ce livre observe la ville dense, plus que la ville-archipel, et la transformation des gares en métaphores de la société de services : lieux de commutations des codes et des rythmes, entre déplacement et consommation, loisir et travail ; temporalités confuses et désynchronisées, entre l'attente, le sommeil et la hâte. Ainsi, explorer les villes en gares, c'est arriver en ville, comme hier, ou revenir régulièrement en ville, comme aujourd'hui. Dans quel morceau de ville ou quel quartier ? Dans quel univers de civilités et selon quel régime d'exposition et de disponibilité ? La gare est un miroir des langages disponibles pour comprendre la notion d'usage et la perception en situation, sans tourner le dos aux enjeux de la cité et de l'action publique. En ce sens, elle entretient l'utopie d'un citoyen qui ferait l'expérience ordinaire d'un étrange accord entre les objets, les signes et les conduites.},
	pagetotal = {308},
	publisher = {Editions de l'Aube},
	author = {Joseph, Isaac},
	date = {1999-05-28},
	langid = {french},
}

@article{bairras_slow_2025,
	title = {The Slow and Difficult Implementation of High Speed Rail Interoperability in Europe: the Case of the Atlantic Corridor},
	volume = {82},
	issn = {2352-1465},
	url = {https://www.sciencedirect.com/science/article/pii/S2352146524005039},
	doi = {10/g83tc7},
	series = {World Conference on Transport Research - {WCTR} 2023 Montreal 17-21 July 2023},
	shorttitle = {The slow and difficult implementation of high speed rail interoperability in Europe},
	abstract = {The European Union ({EU}) has started to create a railway transport network with all the national networks of it member states, in join all them event its differences and building some corridors as priority work. This article presents the current situation of the Atlantic Corridor and the difficulties in integrating the importance of rail interoperability in its development. First, our methodology is presented (section 2). Then, it attempts to define the corridor object and the way in which European governance has set up the {TEN}-T (section 3) by presenting the concept of interoperability as European standardisation (section 4). It analyses the current problem of implementing all the necessary steps to homogenise the various national networks in order to reach a true European Corridor (section 5). To put all these concepts into perspective, we focus on one of the nine corridors proposed in the {TEN}-T: the Atlantic Corridor (section 6). Through its history and development, we present the current status, the difficulties and barriers to its effective implementation and the solutions envisaged to enable the construction of the true continental network (section 7).},
	pages = {2546--2558},
	journaltitle = {Transportation Research Procedia},
