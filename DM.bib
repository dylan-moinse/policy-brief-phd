@article{dovey_isochrone_2017,
	title = {Isochrone Mapping of Urban Transport: Car-dependency, Mode-choice and Design Research},
	volume = {32},
	issn = {0269-7459},
	url = {https://doi.org/10.1080/02697459.2017.1329487},
	doi = {10/g8sbvw},
	shorttitle = {Isochrone Mapping of Urban Transport},
	abstract = {Imperatives to develop more walkable, transit-oriented and low-carbon cities have accentuated the need to understand the performance and possible transformation of urban access networks. Within a framework of complex adaptive systems and assemblage thinking we develop isochrone mapping of urban transport access in four transport modes: walking, cycling, public transport and cars. These isochrones can be used to compare the range and area of urban access for each of these modes, over different time limits and for different times of day. Such mapping gears urban morphology to the phenomenology of urban transport and enables us to better understand mode choice. Through design research we can also test a range of possible design scenarios for infrastructure investment and show their impact on the isochrone maps. We conclude with a focus on the relations between car and public transport isochrones as a means of better understanding car-dependency and what we call the ‘Car/{PT} assemblage’.},
	pages = {402--416},
	number = {4},
	journaltitle = {Planning Practice \& Research},
	author = {Dovey, Kim and Woodcock, Ian and Pike, Lucinda},
	urldate = {2024-11-28},
	date = {2017-08-08},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/02697459.2017.1329487},
	keywords = {Isochrone mapping, assemblage, design research, mode-choice, urban transport, walkability},
}

@thesis{krismer_enhancing_2017,
	title = {Enhancing isochrones in multimodal spatial networks},
	url = {https://ulb-dok.uibk.ac.at/ulbtirolhs/content/titleinfo/2290756},
	abstract = {Isochrones are an important tool for carrying out reachability analyses. In Multimodal Spatial Networks they are hard to compute, since multiple modalities and schedules of the public transportation systems need to be considered. Throughout this thesis new algorithms to compute isochrones in such networks are developed. Those include loading parts of the data source with the help of so called tile regions as well as computations on previously calculated results. The application capabilities of isochrones are extended by defining averaged and time-invariant isochrones. By introducing elevation aware isochrone computation the accuracy of the results is further increased. An approach that is able to tailor isochrones to individual users is presented as well.

    One of the major problems in the field of isochrone computation is the acquisition of the necessary data. To address the lack of datasets on which such calculations can be performed, a generalized dataset creation is introduced that allows not only to create real-world models, but also to include information from external data sources. Data is merged with information about the public transportation system and with Digital Elevation Models that include elevation data. To ease the visualization of computed isochrones in real-world datasets a web-application called "{IsoMap}" is presented. It can be used to vary computation parameters and for basic analyses within the results.},
	pagetotal = {156},
	institution = {Faculty of Mathematics, Computer Science and Physics of the University of Innsbruck},
	type = {phdthesis},
	author = {Krismer, Nikolaus},
	editora = {Günther, Specht},
