@misc{chen_exploring_2022,
	location = {Rochester, {NY}},
	title = {Exploring Feeder-Related Built Environment Effects on Integrated Usage of Taxi and Metro},
	url = {https://papers.ssrn.com/abstract=4142195},
	doi = {10.2139/ssrn.4142195},
	abstract = {Integrated taxi and metro usage is an essential part of transit travel for metro access and egress. This paper measures access and egress distance threshold of the feeding mode for metro transferring and scrutinizes the feeder-related built environment impacts on taxi-metro integration. Regression models are developed to explore the transfer distance of access/egress trips. Then, the catchment area is delineated for measuring feeder-related environment variables. Finally, negative binomial regressions are employed to examine the relationships between the feeder-related built environment and taxi-metro integrated usage. The results reveal that (a) the access-integrated usage is stronger during morning peak hours, while the egress-integrated usage is stronger during evening peak hours; (b) the feeding distance of taxi is about 3.8 km; (c) mixed land use increases morning-egress-integrated usage and evening-integrated usage rates. Residential land use attracts morning-access-integrated usage and evening-integrated usage, while commercial land use is negatively correlated with morning-egress integrated usage.},
	number = {4142195},
	author = {Chen, Qixiang and Lv, Bin and Chen, Xiqun and Hao, Bin-bin},
	urldate = {2022-07-22},
	date = {2022-06-21},
	langid = {english},
	keywords = {Taxi, catchment area, feeder-related environment, feeding mode, metro},
}

