@article{sung_transit-oriented_2011,
	title = {Transit-Oriented Development in a High-Density City: Identifying its Association with Transit Ridership in Seoul, Korea},
	volume = {28},
	issn = {0264-2751},
	url = {https://www.sciencedirect.com/science/article/pii/S0264275110001332},
