@article{roig-costa_disrupted_2024,
	title = {Disrupted intermodality: Examining adaptation strategies to public transport e-scooter bans in Barcelona},
	issn = {1556-8318},
	url = {https://doi.org/10.1080/15568318.2024.2434881},
	doi = {10/g8t72v},
	shorttitle = {Disrupted intermodality},
	abstract = {Electric scooters (e-scooters) have changed urban mobility by offering a dynamic solution to the critical “first and last mile” problem, connecting individuals from their homes to public transport and their final destinations. Despite their growing popularity, e-scooters navigate through a landscape of shifting legal frameworks, highlighting the urgency for policies that not only harness their potential but also address their inherent challenges. This study aims to shed light on the intermodal practices and demographics of e-scooters users in Barcelona, explores the potential impacts of regulatory changes on established transport habits, and assesses the adaptability of users to changing transportation options. Through a self-reported survey of 311 private e-scooter users, we find a notable prevalence of young men from lower socioeconomic backgrounds engaging in intermodal travel, primarily for employment purposes. To better understand how e-scooter riders integrate the device in their daily mobility strategies, we introduce the Intermodality Ratio ({IR}). A Generalized Linear Model ({GLM}) is then used to identify key demographic, socioeconomic, and geographic predictors of the {IR}, revealing place of residence as the most significant factor influencing intermodal behavior. Finally, we analyze participants’ anticipated behavioral shifts in response to the upcoming ban using a Multinomial Logistic Regression ({MLR}) model, which explores the sociodemographic factors affecting the likelihood of adopting alternative transport strategies. These findings contribute to the limited understanding of e-scooter utilization and intermodal practices, particularly within the context of public transit, offering insights into how transport policies can more effectively accommodate emerging mobility solutions.},
	pages = {1--14},
	journaltitle = {International Journal of Sustainable Transportation},
	author = {Roig-Costa, Oriol and Miralles-Guasch, Carme and Marquet, Oriol},
	urldate = {2024-12-10},
	date = {2024-09-12},
	langid = {english},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/15568318.2024.2434881},
	keywords = {E-scooter, intermodality, policy restriction, public transportation, stated preferences},
}

@article{meissonnier_pour_2012,
	title = {Pour mieux analyser les comportements de déplacement, comment ajuster nos protocoles d'enquête ?},
	volume = {62 {\textbar} 2012},
	url = {https://shs.hal.science/halshs-01778193},
	doi = {10/g8t72t},
	abstract = {Suite à une observation participante de la formation d'enquêteurs à l'Enquête Ménages Déplacement ({EMD}), l'auteur pointe des dysfonctionnements dans certaines situations de déplacement. Le protocole d'enquête apparaît mal approprié pour saisir les déplacements dotés d'une utilité intrinsèque (undirected travel). Lorsque le vécu de l'individu mobile, la multiplicité de ses motifs, le surgissement d'intentions en cours de route ou la survenance d'un aléa sur le trajet affectent le comportement de déplacement, des questions plus ouvertes s'imposent. L'auteur plaide pour un croisement des méthodes ; il suggère notamment l'introduction occasionnelle de parenthèses qualitatives et compréhensives au cœur du questionnaire quantitatif quand l'enquêteur constate que la situation de déplacement s'éloigne du schéma classique d'un déplacement dérivé d'une demande d'activité à destination (directed travel). Bien que cet ajustement du protocole supposerait une complexe révision du standard {CERTU} de l'{EMD} et une refonte du mode de recrutement et de formation des enquêteurs, l'auteur a supposé ces obstacles levés pour apprécier les perspectives théoriques qui pourraient en découler. Tout en soulignant la richesse des nuances permises par la modélisation axiale proposée par {MOKHTARIAN} et {SALOMON} (2001), il en montre les limites et tente d'en lever les ambiguïtés. Cet axe comporte huit clés de détermination différentes pouvant constituer l'armature d'une modélisation du comportement de déplacement sur quatre plans.},
	pages = {3},
	journaltitle = {Les Cahiers Scientifiques du Transport / Scientific Papers in Transportation},
	author = {Meissonnier, Joël},
	urldate = {2024-12-09},
	date = {2012-11-30},
	langid = {french},
}

@inproceedings{sebille_saisir_2024,
	location = {Aveiro, Portugal},
	title = {Saisir la diversité des pratiques de mobilité dans les quartiers périphériques populaires des métropoles latino-américaines : l'apport des enquêtes par questionnaire Modural},
	url = {https://hal.science/hal-04690960},
	shorttitle = {Saisir la diversité des pratiques de mobilité dans les quartiers périphériques populaires des métropoles latino-américaines},
	abstract = {Les métropoles d’Amérique latine se caractérisent par de fortes inégalités socio-spatiales, des problèmes de congestion et s’étalent sur de grandes étendues. Les trajets quotidiens y sont difficiles pour une très grande partie de leurs habitants. Les déplacements notamment pour le travail et les études, qui renvoient à des mobilités non reportables et non délégables, représentent un défi quotidien particulièrement pour les habitants des zones excentrées. Les distances parcourues, la complexité des trajets, et leurs durées souvent très longues, sont parmi les principaux marqueurs de déplacements quotidiens harassants. La description et l’analyse de ces mobilités nécessitent une réflexion, tant sur les concepts utilisés que sur les méthodes employées pour collecter des données révélatrices de la diversité des pratiques de mobilité. Un tel travail a été entrepris à l’occasion du projet {ANR} Modural « Les pratiques de la mobilité durable dans les métropoles d’Amérique latine ». La communication présente le dispositif d’enquêtes par questionnaire du projet Modural mis en œuvre en 2022 auprès de 1600 ménages à Bogotá (Colombie) et à Lima (Pérou) dans quatre zones périphériques par ville, ainsi que les défis méthodologiques associés à son élaboration. Elle aborde le choix du niveau statistique retenu pour l’analyse, la distinction entre trajets et étapes, et les innovations apportées au questionnaire du projet. L’enquête a ainsi permis de saisir à la fois les mouvements dits pendulaires ayant des lieux d’origine et de destination fixes, fréquentés au quotidien et les mobilités qui s’éloignent du schéma pendulaire et qui sont associées à des profils de travailleurs mobiles, d’activités parfois informelles.},
	booktitle = {{XXIIème} Colloque de l'{AIDELF} - 2024 Démographie et mobilités},
	publisher = {{AIDELF}},
	author = {Sebille, Pascal and Demoraes, Florent},
	urldate = {2024-12-09},
	date = {2024-08},
	langid = {french},
	keywords = {Bogotá, Enquête ménage, Lima, mobilité, méthode, questionnaire, trajets, étapes},
}

