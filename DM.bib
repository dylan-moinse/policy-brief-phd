	keywords = {A lire et ficher, Systematic\_Review},
}

@inproceedings{hu_study_2019,
	title = {Study on the Cycling Attraction Area of Metro Station Based on Sharing Bike Riding Data: An Empirical Study in Beijing},
	url = {https://ascelibrary.org/doi/10.1061/9780784482292.362},
	doi = {10/gr5q2r},
	shorttitle = {Study on the Cycling Attraction Area of Metro Station Based on Sharing Bike Riding Data},
	abstract = {Primary modes of access to/egress from metro are cycling and walking. The cycling attraction area is calculated by maximum tolerant cycling time from a traveler survey, which varies by different metro stations. To better guide urban rail transit planning and station {TOD} (transit-oriented development) design, the cycling access/egress distance of metro stations must be calculated and the reasonable {TOD} region studied. Based on the sharing cycling data of Mobike in Beijing, analyses show: (1) the average riding access distance to a metro station is 982 m; (2) cycling {OD} are generally concentrated in the area 0–200 m and 600–1,000 away from the metro station; (3) metro stations can be divided into three types based on access/egress cycling; and (4) the division method based on Voronoi principle can be applied to find the boundary of the {TOD} region. We suggest planning and design of {TOD} areas and parking facilities of non-motorized systems.},
	pages = {4194--4206},
	booktitle = {19th {COTA} International Conference of Transportation Professionals},
	author = {Hu, Li and Lu, Huapu},
	urldate = {2023-04-19},
