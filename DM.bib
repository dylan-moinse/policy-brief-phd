@inproceedings{cooke_infrastructure_2016,
	title = {Infrastructure Development : Bicycle-Based Transit-Oriented-Development ({TOD})},
	rights = {Southern African Transport Conference},
	isbn = {978-1-920017-64-4},
	url = {https://repository.up.ac.za/handle/2263/57996},
	shorttitle = {Infrastructure development},
	abstract = {The financial sustainability of the South African urban public transport systems has been brought into question. The primary determinant of non-viability is the unique spatial structure of South African cities and the associated lack of public transport supportive land use environments. Transit Oriented Development ({TOD}) is being mooted as a potential method to improve inefficient urban forms and the viability of the public transport systems. Prior studies on density articulation have illustrated that the ability of {TOD} to decrease feeder service reliance has a significant positive impact on the viability of the public transport system as a whole. However, {TOD} is confined to the Station Influence Area ({SIA}) of each trunk service station. Therefore, the reach of {TOD} and its effect on public transport viability is dependent upon the population density within each {SIA} and the size of the {SIAs}. Conventionally, an {SIA} is measured as the walking catchment area of a station. If sufficient cycling infrastructure is provided, bicycle-based {TOD} (B-{TOD}) could develop a considerably larger {SIA}, however, with all the associated benefits of conventional pedestrian-based {TOD}. This presentation will illustrate that the positive effects of B-{TOD} on the viability of a public transport service, through decreased feeder service reliance, could far outweigh the costs of the cycling infrastructure. The study utilises a simplified public transport corridor operating cost model to test the effects of varying degrees of B-{TOD} in a generic South African urban context.},
	booktitle = {Southern African Transport Conference},
	author = {Cooke, Sean},
	urldate = {2022-05-30},
	date = {2016},
	langid = {english},
	note = {Accepted: 2016-11-14T07:01:07Z
Artwork Medium: {PDF}
Interview Medium: {PDF}},
	keywords = {A lire et ficher},
}

@article{sengul_impacts_2021,
	title = {Impacts of E-Micromobility on the Sustainability of Urban Transportation—A Systematic Review},
	volume = {11},
	rights = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2076-3417},
	url = {https://www.mdpi.com/2076-3417/11/13/5851},
	doi = {10/gph5qs},
	abstract = {When considering the sharp growth rate of the use of e-micromobility vehicles, such as e-scooters and e-bikes, it is necessary to investigate whether these emerging modes of transport play a sustainable role in cities in terms of their energy efficiency, emissions, and their relationship with other modes of mobility, such as public transport. This paper aims to provide a comprehensive overview of the impacts of e-micromobility through a systematic review of relevant studies in the field of e-scooters and e-bikes. We followed the steps of {PRISMA} to conduct a systematic literature review, including identification, screening, eligibility and inclusion steps. One hundred forty-six studies were reviewed and compiled, and 29 of these studies were selected for the focus of this review and their research data were synthesized. The impacts of e-micromobilities were categorized into four categories—travel behaviors, energy consumption, environmental impacts, and safety and related regulations. The category of travel behaviors includes the analysis of the purposes of travel, modal shift from different modes of transport to e-micromobility vehicles, average travel time, and distance. In this review, the findings of relevant studies in different cities around world are compared to each other and synthesized to give an insight into the role of e-micromobility in the present and in the future of urban transportation.},
	pages = {5851},
	number = {13},
	journaltitle = {Applied Sciences},
	author = {Şengül, Buket and Mostofi, Hamid},
	urldate = {2022-02-21},
	date = {2021-01},
	langid = {english},
	note = {Number: 13
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {Lu (à ficher), e-bike, e-micromobility, e-scooter, mobility behaviors, sustainable mobility, systematic literature review, urban transportation},
}

