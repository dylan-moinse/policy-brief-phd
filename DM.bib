@article{ballouk_analyse_2021,
	title = {Analyse bibliométrique de la littérature portant sur la co-thématique : responsabilité sociale des entreprises et développement durable},
	volume = {6},
	issn = {2262-7030},
	url = {https://www.cairn.info/revue-questions-de-management-2021-6-page-87.htm},
	doi = {10/g848bn},
	series = {{EMS} Editions},
	abstract = {Les liens entre la responsabilité sociale des entreprises ({RSE}) et le développement durable ({DD}) ont fait l’objet de plusieurs travaux académiques et la recherche dans ce domaine ne cesse de proliférer. Cependant, la recherche co-thématique est encore assez nouvelle et n’a pas été entièrement étudiée. Une analyse bibliométrique approfondie utilisant le logiciel R est appliquée pour analyser et visualiser la carte des connaissances de la recherche {RSE} liée au {DD}. Les principaux résultats montrent que l’implication de la {RSE} dans le développement durable est à la fois un sujet de recherche récent, dynamique et prospère. Le réseau des co-auteurs est fragmenté, tandis que les coopérations transnationales se produisent en groupes. Cet article présente un nouvel aperçu de la production scientifique à travers l’analyse bibliométrique (Bibliometrix). Un total de 1469 articles publiés entre 2000 et 2021 ont été analysés. Les principaux résultats de cette recherche ont montré que la responsabilité sociale des entreprises est l’une des variables les plus décisives du développement durable et que des mots-clés tels que la performance, la gouvernance et le management sont étroitement liés. Les résultats fournissent des indices pour une enquête plus approfondie dans ce domaine.},
	pages = {87--99},
	number = {36},
	journaltitle = {Question(s) de Management},
	author = {Ballouk, Houssein and Ben Jabeur, Sami and Ben Arfi, Wissal},
