@article{heran_nouvelles_2018,
	title = {Les nouvelles formes de la mobilité : trottinettes électriques, hoverboards, bicyclettes électriques...},
	volume = {Mai 2018},
	issn = {1148-7941},
	url = {https://www.cairn.info/revue-realites-industrielles-2018-2-page-36.htm},
	doi = {10/ghdkvj},
	shorttitle = {Les nouvelles formes de la mobilité},
	abstract = {Depuis quelques ann\&\#233;es, de nouvelles formes l\&\#233;g\&\#232;res de mobilit\&\#233; urbaine se d\&\#233;veloppent, utilisant des mat\&\#233;riels innovants ou des solutions anciennes revisit\&\#233;es, avec de nombreuses variantes et des appellations commerciales foisonnantes. La presse croit d\&\#233;celer dans ce ph\&\#233;nom\&\#232;ne un nouveau style de glisse urbaine, une mobilit\&\#233; alternative ou une micromobilit\&\#233; qui r\&\#233;volutionnerait nos modes de vie, ou tout du moins celui des citadins les plus branch\&\#233;s ({JACQU}\&\#201;, 2016).Beaucoup moins gourmands en \&\#233;nergie et en mat\&\#233;riaux que les voitures individuelles et produisant peu de nuisances, ces \&\#171;\&\#160;engins de d\&\#233;placement personnel\&\#160;\&\#187; ({EDP}), auxquels s\&\#8217;ajoutent les v\&\#233;los de tous types, m\&\#233;ritent que l\&\#8217;on s\&\#8217;y attarde. Ils sont cependant toujours mis sur le m\&\#234;me plan, comme s\&\#8217;ils avaient tous autant de chances de se d\&\#233;velopper. Pour appr\&\#233;cier leur pertinence \&\#224; long terme, les \&\#233;tudes de march\&\#233; sont peu adapt\&\#233;es, car, en reposant sur l\&\#8217;observation des ventes pass\&\#233;es et sur des enqu\&\#234;tes aupr\&\#232;s de la client\&\#232;le, elles ont tendance \&\#224; surestimer le march\&\#233; potentiel et \&\#224; consid\&\#233;rer comme durable ce qui ne rel\&\#232;ve que d\&\#8217;un simple effet de mode. Pour mieux comprendre les avantages et les limites de chaque solution existante, ou m\&\#234;me potentielle, il est pr\&\#233;f\&\#233;rable de passer en revue les principaux choix techniques \&\#224; r\&\#233;aliser pour innover dans ce domaine, puis d\&\#8217;appr\&\#233;cier les cons\&\#233;quences de ces choix sur la performance de ces diff\&\#233;rents modes de d\&\#233;placement et, enfin, de proposer une typologie de ces formes l\&\#233;g\&\#232;res de mobilit\&\#233;.},
	pages = {36--40},
	number = {2},
	journaltitle = {Annales des Mines - Realites industrielles},
	author = {Héran, Frédéric},
	urldate = {2021-03-16},
