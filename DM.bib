	keywords = {Liseuse, Rail and bus rapid transit, Transit frequency, Transit oriented development ({TOD}), Urban development, Urban regeneration},
}

@article{lee_bicycle-based_2016,
	title = {Bicycle-Based Transit-Oriented Development as an Alternative to Overcome the Criticisms of the Conventional Transit-Oriented Development},
	volume = {10},
	issn = {1556-8318},
	url = {https://doi.org/10.1080/15568318.2014.923547},
	doi = {10/gntfvc},
	abstract = {In order to improve the effectiveness of transit-oriented development ({TOD}), the integration of public transportation and bikes has been regarded as a good alternative because the catchment area of a station, that is, the spatial extent of {TOD}, can be increased. This paper suggests the concept of bicycle-based {TOD} (B-{TOD}) and estimates the spatial extent, considering the catchment area. For this, a trip survey was conducted among bicycle users who used bicycle and public transportation in Seoul metropolitan and Daejeon metropolitan areas in Korea. Regression and cumulative distribution models were used to estimate the spatial extent of B-{TOD}. The results show that the estimated access distances were 1.96 km and 2.13 km for origin (home)-to-station and station-to-work trips, respectively, and they can play a yardstick role in the {TOD} planning practices in Korea. In such cases, the catchment areas of B-{TOD} cover 73.7\% and 93.6\% of the whole area of Seoul, whereas conventional walking-only {TOD} can cover only 29.9\% of the same. This implies that B-{TOD} can help to solve one of the key inherent weaknesses of the conventional {TOD}. Some limitations and recommendations for future research are also discussed.},
	pages = {975--984},
	number = {10},
	journaltitle = {International Journal of Sustainable Transportation},
	author = {Lee, Jaeyeong and Choi, Keechoo and Leem, Yountaik},
	urldate = {2022-02-24},
	date = {2016-11-25},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/15568318.2014.923547},
	keywords = {Access mode of public transportation, B-{TOD}, Revue de litterature, Systematic\_Review, {TOD}, spatial extent, station impact area},
}

