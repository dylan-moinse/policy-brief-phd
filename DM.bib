@book{hayer_fabriquer_2005,
	location = {Paris},
	title = {Fabriquer la ville autrement},
	isbn = {978-2-281-19264-3},
	abstract = {Qu'est-ce qu'un aménageur urbain et quelles sont ses missions ? C'est à ces questions que cet ouvrage, à travers la figure de Jean-Paul Baïetto, tente de répondre. Percevant très tôt les mutations de la société française, ce dernier a pu en tirer des conséquences sur la conception même de sa pratique professionnelle : pour lui, la ville est faite avant tout pour les hommes. Les trois grandes opérations auxquelles il a participé sont emblématiques de l'évolution de l'urbanisme en France entre 1970 et 2000 : Marne-la-Vallée, l'une des plus importantes « villes nouvelles » de la région parisienne ; puis la Villeneuve de Grenoble-Échirolles, projet urbain social presque utopique ; enfin Euralille, symbole du renouveau de la région lilloise. À chaque fois, répondant aux desiderata parfois contradictoires des politiques et défendant les projets souvent audacieux des architectes, il a bataillé avec enthousiasme pour que les programmes se réalisent. À partir de 1993, à la tête du Club Ville-Aménagement, il a contribué à faire évoluer la profession d'aménageur. Il a montré que celui-ci devait être une force de proposition intervenant en amont du projet urbain et sur le long terme, dans une maîtrise d'ouvrage élargie à la conception du projet. Ce livre n'est ni un traité d'aménagement ni une biographie : son objectif est de retracer les questionnements et les réponses que Jean-Paul Baïetto a formulés et de montrer la pertinence et l'actualité de sa démarche. À travers celle-ci, un portrait de l'aménageur urbain idéal se dessine.},
	pagetotal = {223},
	publisher = {Le Moniteur},
	author = {Hayer, Dominique},
	date = {2005-03-24},
}

