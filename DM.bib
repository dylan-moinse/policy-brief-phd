@article{cotton_using_2010,
	title = {Using Observational Methods to Research the Student Experience},
	volume = {34},
	issn = {0309-8265},
	url = {https://doi.org/10.1080/03098265.2010.501541},
	doi = {10/b4xj2f},
	abstract = {Much pedagogic research undertaken in geography and other disciplines relies on post-hoc methods such as surveys or interviews to investigate the student experience of higher education (often based on self-reports of behaviour). However, observation of students provides a far more direct route to obtain information about their behaviour, and there are many situations where observational data—collected by a researcher or by students themselves using video, audio or written diaries—may provide a deeper insight into their experience. This paper outlines some basic methods of observation, describes the potential advantages and disadvantages of collecting observational data, and provides examples of relevant research which draws on these methods. We conclude with a recommendation that observational methods should be used more widely in pedagogic research.},
	pages = {463--473},
	number = {3},
	journaltitle = {Journal of Geography in Higher Education},
	author = {Cotton, Debby R. E. and Stokes, Alison and Cotton, Peter A.},
	urldate = {2022-08-12},
