@inproceedings{tillous_retour_2008,
	title = {Retour sur le « tournant pragmatique » de la notion de « terrain »},
	url = {https://halshs.archives-ouvertes.fr/halshs-00357429},
	abstract = {La notion de terrain comme point de connexion du théorique et de l'empirique a connu une rupture épistémologique fondamentale au cours des années 1980. Les courants post-structuralistes se développent alors dans toutes les disciplines du social en balayant la structure au profit de la singularité, la permanence de la vérité au profit de la contingence et du devenir de l'action. François Dosse a mis en évidence ce « tournant pragmatique » dans son ouvrage L'empire du sens [1995].{\textless}br /{\textgreater}{\textless}br /{\textgreater}Or, les sciences de l'espace sont paradoxalement très peu évoquées par l'historien. Son travail se focalise sur les disciplines historiques, sociologiques et anthropologiques, qu'il regroupe sous le terme de "sciences humaines" pour pouvoir les confronter aux sciences exactes et à la philosophie. Ainsi, la géographie est absente de ce travail, mais également les incursions spatiales des disciplines citées. Par exemple, Michel de Certeau, qui dans la troisième partie de L'invention du quotidien – 1. Arts de faire [{DE} {CERTEAU}, 1980] se consacre à l'étude des « Pratiques d'espace », est cité à plusieurs reprises par François Dosse, mais jamais à propos de questions spatiales. Au contraire, l'historien affirme : « cette orientation [de la discipline historique] doit beaucoup à Michel de Certeau qui a étudié les pratiques quotidiennes d'appropriation qui ont pour caractéristique d'être éphémères, instables, sans lieu. » [{DOSSE}, 1995 : 257-258, souligné par nous].{\textless}br /{\textgreater}{\textless}br /{\textgreater}Nous nous sommes proposé de combler ce manque théorique en réalisant une lecture croisée de l'analyse historique de François Dosse et d'un document témoin de ce tournant dans les sciences de l'espace : l'ouvrage de Jean-François Augoyard intitulé Pas à Pas [1979] sur lequel s'appuie précisément Michel de Certeau pour la rédaction du huitième chapitre des Arts de faire, consacré aux « Marches dans la ville » [{OFFNER}, 1981 : 67]. L'enjeu de cette lecture étant de caractériser le « tournant pragmatique » de la notion de « terrain » : quelles sont les similitudes et les spécificités du changement de paradigme au sein des sciences de l'espace au regard des autres sciences sociales ?{\textless}br /{\textgreater}{\textless}br /{\textgreater}{AUGOYARD}, J.-F. 1979. Pas à pas. Essai sur le cheminement quotidien en milieu urbain. Paris, Seuil, 190p.{\textless}br /{\textgreater}{DE} {CERTEAU}, M. 1980. L'invention du quotidien. t.1. Arts de faire. Paris, {UGE}, « 10/18 ».{\textless}br /{\textgreater}{DOSSE}, F. 1995. L'empire du sens. L'humanisation des sciences humaines. Paris, La Découverte, 432p.{\textless}br /{\textgreater}{OFFNER}, J.-M. 1981. « Les déplacements piétonniers. Analyse bibliographique. » Institut de recherche des transports : Note d'information, n° 20.},
	eventtitle = {À travers l'espace de la méthode : les dimensions du terrain en géographie},
	author = {Tillous, Marion},
	urldate = {2022-09-21},
	date = {2008-06},
	langid = {french},
}

